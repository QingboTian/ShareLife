<view class="container">
  <view class="feedbackList">
    <i-cell-group>
      <block wx:for="{{feedbackList}}" wx:for-item="feedback" wx:key="index">
        <i-cell title="{{feedback.titlte}}" label='{{feedback.createtime}}' value="{{feedback.status ? '已处理':'未处理'}}" is-link url='../showFeedback/showFeedback?id={{feedback.id}}'></i-cell>
      </block>
    </i-cell-group>
  </view>
  <view>
    <i-cell-group>
      <i-cell title="我要反馈" is-link url="../feedback/feedback"></i-cell> 
    </i-cell-group>
  </view>
</view>
